
page{background-color:#f8fcfb;height: 100%;width: 100%;}
.head{width: 100%;height: 350rpx;text-align: center;position: relative}
.img9{width:100%;height: 350rpx;}
.head image.img10{margin-bottom: 22rpx;border-radius:50%;position: absolute;left: 300rpx;top: 43rpx;}
.head .userinfo-nickname{color: #000;font-size: 30rpx;display: block;width: 100%;position: absolute;top: 200rpx;}
.head button.tou{position: relative;top:43rpx;width: 140rpx;height: 140rpx;border-radius:50%;border:none}
button::after{border:none}
.head button.name{width: 380rpx;height: 80rpx;line-height: 80rpx;position: absolute;top: 200rpx;left: 25%;font-size: 30rpx;background-color: #44b29e;color:#303030}
.head .get{position: absolute;top: 210rpx;font-size: 30rpx;color: #303030;right:33%}

.point{width: 100%;height: 215rpx;text-align: center;border-bottom:1px solid #dedede;}
.point image{width: 70rpx;height: 70rpx;display: block;margin: 55rpx auto 30rpx;}
.point .appointment{width: 33.33%;float: left;text-align: center;height: 100%;position: relative}
.point .consult{width: 33.33%;float: left;text-align: center;height: 100%}
.point .notice{width: 33.33%;float: left;text-align: center;height: 100%}
.point text{font-size: 28rpx;color: #494949}

.points{width: 18rpx;height: 18rpx;background-color: red;border-radius:50%;float: right;margin-right:70rpx;margin-top:45rpx;}
