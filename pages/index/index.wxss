page{background-color: #fff}
.doctorList{width: 100%;height: 170rpx;line-height: 170rpx; z-index: 33;}
.img{float: left;margin-top: 24rpx;}
.kkk{border-bottom:1px solid #dedede;height: 170rpx;line-height: 170rpx; z-index: 1;overflow: hidden;margin-left: 30rpx;}
.doctorList .img image{border-radius:50%;}
.doctorList .doctorName{float: left;margin-left: 20rpx;font-size: 30rpx;}
.doctorList button{width: 140rpx;height: 50rpx;display: inline-block;border-radius: 25rpx;border: 1px solid #44b29f;letter-spacing: 2rpx;text-align: center;font-size: 28rpx;line-height: 50rpx;background-color: #fff}
.doctorList button.b1{float: right;margin-right:30rpx;margin-top:60rpx;}
.doctorList button.b2{float: right;margin-right: 30rpx;margin-top:60rpx;}

modal{height:800rpx;text-align:center;width: 280rpx;}   
modal image{width: 100rpx;height: 100rpx;} 
.anniu button{width: 50%;position:absolute;right:0;z-index: 999;height: 100rpx;font-size: 38rpx;color: green} 
.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(0,0,0,0.7);
  overflow: hidden;
  z-index: 999;
  text-align: center;
}
.modal-body{
  position:relative;
  }
.modal-content{
  margin:280rpx 10% 0 10%;
  overflow: hidden;
  background-color: #fff;
  height: 400rpx;
  border-radius:10rpx; 
}
.modal-box .login{font-size: 32rpx;text-align: center;font-weight: 600;padding-top: 25rpx;}
.modal-box image{width: 100rpx;height: 100rpx;margin-top: 50rpx;border-radius:50%;}
.modal-box .hosp{font-size: 30rpx;margin-top: 20rpx;}
.modal-box button.comfir{height: 100rpx;background-color: #fff;border:none;font-size: 29rpx;margin-top: 30rpx;line-height: 87rpx;color: green}
